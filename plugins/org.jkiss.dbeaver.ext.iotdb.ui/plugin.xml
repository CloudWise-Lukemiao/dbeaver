<plugin>



    <extension point="org.jkiss.dbeaver.dataManager">
<!--
        <manager class="com.dbeaver.eeinfluxdb.data.managers.InfluxDBDocumentValueManager" id="com.dbeaver.eeinfluxdb.data.managers.InfluxDBDocumentValueManager">
            <supports type="java.lang.Object"/>
        </manager>
