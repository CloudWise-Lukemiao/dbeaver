    </extension>

    <extension point="org.eclipse.ui.menus">
        <menuContribution allPopups="false" locationURI="menu:dataSourceMenu?after=transactions">

            <menu label="%command.com.dbeaver.ee.influxdb.consistencyLevelSelect.name">
                <dynamic id="com.dbeaver.ee.influxdb.consistencyLevelMenu" class="org.jkiss.dbeaver.ext.iotdb.views.IoTDBConsistencyLevelContributor"/>
                <visibleWhen>
                    <with variable="activeEditor">
                        <adapt type="org.jkiss.dbeaver.model.DBPDataSourceContainer">
                            <test property="org.jkiss.dbeaver.core.datasourceContainer.driverId" value="iotdb"/>
                        </adapt>
                    </with>
                </visibleWhen>
            </menu>

        </menuContribution>
    </extension>
   

</plugin>
